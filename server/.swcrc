{
    "jsc": {
        "parser": {
            "syntax": "typescript",
            "tsx": false,
            "dynamicImport": true,
            "decorators": false
        },
        "target": "es2020",
        "loose": false,
        "externalHelpers": false,
        "keepClassNames": false,
        "baseUrl": "./",
        "paths": {
            "@/*": [
                "./src/*"
            ],
            "*": [
                "node_modules"
            ]
        }
    },
    "module": {
        "type": "nodenext",
        "strict": false,
        "strictMode": false,
        "noInterop": false,
        "lazy": false
    },
    "minify": true
}